//Solution link : https://leetcode.com/problems/minimum-operations-to-exceed-threshold-value-i/solutions/5684756/very-easy-java-solution-beats-100/
// java solution

class Solution {
    public int minOperations(int[] nums, int k) {
        
        int c=0;
        for(int i=0;i<nums.length;i++){
           if(nums[i]<k){
                 c++;
           }
        }
        return c;
        
    }
}
